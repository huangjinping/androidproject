<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.cooperlink.privatetutor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" >
    </uses-permission>
    <uses-permission android:name="android.permission.READ_LOGS" >
    </uses-permission>
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:name="cn.cooperlink.privatetutor.base.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.cooperlink.privatetutor.main.activity.IndexActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.login.activity.RegisterActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.login.activity.LoginActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.tutor.FindTutorActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.community.CommunityActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.questions.QuestionsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.demand.activity.DemandActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.tutor.SearchActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.community.PostingActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.login.activity.ChangePasswordActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.appointment.activity.PurchaseCourseActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.students.activity.StudentsActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden|adjustPan" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.main.activity.IntegralActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.main.activity.ShareActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="cn.cooperlink.privatetutor.community.TopicDetailsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name="cn.cooperlink.privatetutor.questions.AskQuestionsActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>